{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "@sunner/ambi": ["./src/index"],
      "@sunner/ambi/*": ["./src/*"]
    },
    "rootDir": ".",
    "types": ["vite/client"]
  }
}
